* {
  margin: 0;
  padding: 0;
  box-sizing: border-box;
  touch-action: manipulation;
}
:root {
  --header-blue-: #0b266f;
  --paragraph-blue: #00144a;
  --product-gray-bg: #edeff5;
  --light-gray-bg: #1a368312;
  --hr-gray: #00000014;
}

body {
  font-family: "Roboto", sans-serif;
  overflow-x: hidden !important;
  box-sizing:border-box;
}
a,
ul,
li {
  text-decoration: none;
  list-style-type: none;
  color: var(--header-blue-);
  padding: 0 !important;
  margin: 0 !important;
}
a:visited {
  color: var(--header-blue-);
}
img {
  width: 100%;
}
h1,
h2,
h3 {
  font-family: "Funnel Display", sans-serif;
  color: var(--header-blue-);
}
h1,
h2,
h3,
h4,
h5,
h6,
p {
  margin: 0 !important;
  padding: 0 !important;
}
h1 {
  /* font-size: 3.75rem !important; */
  font-size: clamp(2.25rem, 1.9231rem + 1.5385vw, 3.75rem) !important;
}
h2 {
  font-size: clamp(1.5rem, 1.1731rem + 1.5385vw, 3rem) !important;
}
h3 {
  font-size: clamp(1.125rem, 1.0433rem + 0.3846vw, 1.5rem) !important;
}
p {
  color: var(--paragraph-blue);
  font-size: clamp(0.875rem, 0.8478rem + 0.1282vw, 1rem) !important;
}

.funnel-font {
  font-family: "Funnel Display", sans-serif !important;
}
.roboto-font {
  font-family: "Roboto", sans-serif !important;
}
.custom-container {
  padding-left: clamp(1rem, 11.119vw - 1.953rem, 11.25rem) !important;
  padding-right: clamp(1rem, 11.119vw - 1.953rem, 11.25rem) !important;
}
 .text-main {
  color:var(--header-blue-)
 }
.bg-main-blue {
  background-color: var(--header-blue-) !important;
}
.btn-main {
  background-color: var(--header-blue-);
  color: white;
  font-family: "Funnel Display", sans-serif;
  width: fit-content;
  padding: 1.125rem 3.375rem !important;
  border-radius: 8px !important;
  border: none;
}
.btn-white {
  background-color: white;
  color: var(--header-blue-);
  border: none;
  font-family: "Funnel Display", sans-serif;
  font-weight: bold;
  width: fit-content;
  padding: 1.125rem 3.375rem;
  border-radius: 8px !important;
}
.section-padding {
  padding-top: 5.625rem;
}
.section-margin {
  margin-top: 5.625rem;
}
.w-fit {
  width: fit-content !important;
}
.text-discount {
  color: #06a254;
  font-weight: 600;
}
.bg-gray {
  background-color: var(--product-gray-bg) !important;
}
.dot-decorate {
  list-style-type: disc !important;
}
.small-text {
  font-size: 12px;
  opacity: 0.8;
}
.p-12 {
  padding: 12.5px 0;
}
.p-14 {
  padding: 14px;
}
.rounded-8 {
  border-radius: 8px !important;
}
.rounded-12 {
  border-radius: 12px !important;
}
.underline {
  text-decoration: underline !important;
  text-underline-offset: 2px;
}
.top-15 {
  top:15% !important;
}
/* NAV */
.overlay {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 20;
  cursor: pointer;
}
.hamburger {
  max-width: fit-content;
  margin-right: 10px;
}
.line {
  display: block;
  height: 2px;
  width: 20px;
  background-color: var(--header-blue-);
  transition: all 0.15s ease-in-out;
}
#fluturaNavLinks {
  color: var(--header-blue-);
}
#fluturaNavLinks a {
  white-space: nowrap;
}
#nav-logo {
width:90px ;
  margin-right: 0px;
}
#cart-svg {
  width: 15px;
}

.searchbar-container {
  width: 100%;
}
#searchBar {
  border-top-right-radius:8px !important;
  border-bottom-right-radius:8px !important;
}
@media (min-width: 1600px) {
  .searchbar-container {
    width: 400px;
  }
}

.info-icon-container,
 .info-icon {
  cursor: pointer;
}


.info-text {
  opacity: 0;
  user-select: none;
  pointer-events: none;
  position: absolute;
  top: -100%;
  left: 122px;
  width: 200px;
  padding:10px;
  background-color:white;
  border-radius: 12px;
}
.info-icon-container:hover .info-text {
  opacity: 1;
  pointer-events: auto;
}
nav .input-group-text,
nav .form-control {
  background-color: var(--product-gray-bg) !important;
  border: none;
}
nav .form-control::placeholder {
  opacity: 0.36;
  color: var(--paragraph-blue);
}
nav .form-control:focus-visible,
nav .form-control:focus {
  outline: none;
  border: none;
  box-shadow: none;
}
.cart-nav {
  min-width: 100px;
}
.displaySearchBar {
  display: flex !important;
  position: fixed;
  left: 50%;
  top: 180px;
  transform: translate(-50%, -50%);
  width: 80svw !important;
  border-radius: 8px !important;
  border-top-left-radius: 8px !important;
  z-index: 30;
  max-width: 800px;
}
/* Cart Container */
.cartContainer {
  width: 700px;
  padding: 2rem;
  height: 100dvh; 
  position: fixed;
  top: 0;
  right: -1000px;
  z-index: 10000;
  transition: right 0.2s ease-in-out;
}

/* When the cart is active */
.cartActive {
  top: 0;
  right: 0;
}

/* Make the top section (header + items) flex and fill available space */
.cartContainer > div:first-child {
  flex: 1;
  display: flex;
  flex-direction: column;
  max-height:550px;
}

/* Let the cart items area take available space and scroll if needed */
.cartItemsContainer {
  flex: 1;
  overflow-y: auto !important;
  /* Remove any fixed height constraints */
}

/* Cart info remains at the bottom */
.cart-info {
  margin-top: auto;
}

/* (Keep your other styles as needed) */
.cartItemContainer .prodImage {
  max-width: 220px;
  max-height: 200px;
  height: 100%;
  width: 100%;
  background-color: var(--product-gray-bg);
  padding: 0.725rem;
}
.cartItemContainer .prodImage img {
  object-fit: contain;
}
.quantity {
  width: 150px;
  border: 1px solid #0b266f3d;
}
#closeBtn {
  width: 20px;
  height: 20px;
}
#quantityBtnM {
  width: 20px;
  height:2px;
}
#quantityBtnP {
  width: 20px;
  height:20px;
}
#discount-code {
  border: 1px solid #0b266f3d;
  padding: 0.725rem 1.2rem;
  max-width: 300px;
}
#discount-code::placeholder {
  opacity: 0.5;
}
.thumbnailSearch {
  width: 100px;
  height: 100px;
}
.prod-card button {
  background-color:transparent;
  border: none;
  color:var(--header-blue-);
  font-weight:bold;
}
input[type="number"] {
  appearance:none;
}
/* Hero Section */
.hero-Content {
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}
/* Prod */
.prod-image {
  position: relative;
  background-color: var(--product-gray-bg);
  border-radius: 8px;
  padding: 10px !important;
  height: 225px;
  width: 100%;
  padding: 10px 0;
}
.prod-image img {
  height: 100%;
  object-fit: contain;
}
.perPiece {
  font-size: 16px;
  font-weight: 400;
  color: #00144a;
  opacity: 0.8;
  display: block;
  margin-top: 5px !important;
}
#title {
  margin-top: 10px !important;
  font-size: 1rem;
  letter-spacing: 0.38px;
  text-transform: uppercase;
}

#orderBulk {
  background-image: url("/assets/images/png/tp-pink-bg-hq.png");
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  padding: 5rem;
}
#orderBulk p {
  max-width: 767px;
}
/* Show Case */
.showCaseImg {
  display: flex;
  align-items: center;
  justify-content: center;
}
.showCaseImg img {
  height: 100%;
  width: 100%;
  object-fit: cover;
  border-radius: 8px;
}
.cardContainer {
  background-color: var(--product-gray-bg);
  border-radius: 8px;
  padding: 1.5rem;
  max-width: 310px;
  margin: 0 auto;
}
.cardImage {
  background-color: white;
  padding: 0.5rem;
  border-radius: 8px;
}
.cardImage img {
  width: 38px;
}
/* Zero plastic */
#zeroPlastic {
  background-image: url("/assets/images/png/forest-hq.png");
  background-position: center;
  background-repeat: no-repeat;
  background-size: cover;
  padding: 8rem;
}
#zeroPlastic h1 {
  max-width: 674px;
}
/* Footer */
.socialLogo {
  width: 24px;
  height: 24px;
}

/* CATEGORY PAGE */
#categoryHeader {
  background-color: var(--product-gray-bg);
}
.paginationContainer span,
.paginationContainer a {
  padding: 0.75rem 2rem !important;
  border: 1px solid transparent;
}

.paginationContainer span:hover,
.paginationContainer a:hover {
  border-radius: 8px;
  border: 1px solid var(--header-blue-);
}

/* Always show a border for the active/current page */
.paginationContainer span.current {
  border: 1px solid var(--header-blue-);
  border-radius: 8px;
}

.next img,
.prev img {
  width: 20px;
  height: 20px; 
}
.image-Nav img {
  width: 240px;
  height: 140px;
  object-fit: contain;
}

/* PRODUCT PAGE */

.singleProdContainer {
  background-color: var(--product-gray-bg);
  border-radius: 8px;
  padding: 1rem;
  height: 629px;
  max-height: 80svh;
}

.singleProdContainer img {
  object-fit: scale-down;
  height: 100%;
}
.thumbnailProdContainer {
  height: 100px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.thumbnailProdContainer img {
  object-fit: contain !important;
  width: 100%;
  margin: auto 0;
  border-radius: 4px;
}

#thumbnailProductShowCase {
  max-height:80svh;
}

#thumbnailProductShowCase .splide__arrow {
  background: none;
  border: none;
}
#thumbnailProductShowCase .splide__arrow--prev svg {
  fill: var(--header-blue-);
  transform: rotate(270deg) translateY(-8px);
  width: 15px;
  position: absolute;
  left:50%;
  top: -40px;
  z-index: 30;
}
#thumbnailProductShowCase .splide__arrow--next svg {
  fill: var(--header-blue-);
  transform: rotate(90deg) translateY(-8px);
  width: 15px;
  position: absolute;
  right: 50%;
  top: 100%;
  z-index: 30;
}
#thumbnailProductShowCase .splide__list {
  /* display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center !important; */
  
}
#thumbnailProductShowCase .splide__slide {
opacity: 0.6;
}
#thumbnailProductShowCase .is-active {
opacity: 1;
  border: 2px solid var(--header-blue-);
  border-radius: 4px;
}
