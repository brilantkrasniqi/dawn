/* Loader Animation */
.loader {
  width: 25px;
  height: 25px;
  border-radius: 50%;
  border: 3px solid;
  border-color: #1A3683 #0000;
  animation: l1 1s infinite;
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 10;
}
@keyframes l1 {to{transform: translate(-50%, -50%) rotate(.5turn)}}

